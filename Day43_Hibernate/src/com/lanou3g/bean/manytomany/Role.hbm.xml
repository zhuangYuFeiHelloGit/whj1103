<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class
        name="com.lanou3g.bean.manytomany.Role"
        table="role">
        <id name="rid">
            <generator class="uuid"/>
        </id>
        <property name="rname"/>
        <set name="users"
                cascade="all">
            <key column="uid"/>
            <many-to-many
                class="com.lanou3g.bean.manytomany.User"/>
        </set>
    </class>
</hibernate-mapping>