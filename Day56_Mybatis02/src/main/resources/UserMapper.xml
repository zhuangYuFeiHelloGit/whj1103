<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user">

    <select id="findById"
            parameterType="int"
            resultType="com.lanou.domain.User">
        SELECT * FROM USER WHERE id=#{id}
    </select>



</mapper>